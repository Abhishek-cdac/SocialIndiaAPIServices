<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.pack.commonvo.CountryMasterTblVO" table="country_mst_tbl">
		<id name="countryId" column="COUNTRY_ID" type="integer">
			<generator class="native"></generator>
		</id>
		<property name="countryName" column="COUNTRY_NAME" type="string" length="50"></property>
		<property name="alphaCode" column="ALPHA_CODE" type="string" length="5"></property>
		<property name="numericCode" column="NUMERIC_CODE" type="integer" ></property>		
	</class>
</hibernate-mapping>