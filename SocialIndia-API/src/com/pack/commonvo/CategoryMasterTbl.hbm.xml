<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.pack.commonvo.CategoryMasterTblVO" table="category_mst_tbl">
		<id name="iVOCATEGORY_ID" column="CATEGORY_ID" type="integer">
			<generator class="native"></generator>
		</id>
		<property name="iVOCATEGORY_NAME" column="CATEGORY_NAME" type="string" length="60"></property>
		<property name="iVOACT_STAT" column="ACT_STAT" type="integer" length="11"></property>
		<property name="categoryImageName" column="CATEGORY_IMAGE_NAME" type="string" length="50"></property>
		<property name="categoryDescription" column="CATEGORY_DESCP" type="text"></property>
		<property name="iVOCTG_SHTFORM" column="CTG_SHTFORM" type="string" length="50"></property>
		<property name="ivrCatetype" column="CATEGORY_TYPE" type="integer" length="11"></property>
	</class>
</hibernate-mapping>