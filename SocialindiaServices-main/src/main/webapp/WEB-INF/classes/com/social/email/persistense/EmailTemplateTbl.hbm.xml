<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping >
<class name="com.social.email.persistense.EmailTemplateTblVo" table="email_temp_mst_tbl">

<id name="tempid" column="TEMPLATE_ID">
<generator class="native"></generator>
</id>
<property name="tempName" column="TEMPLATE_NAME" type="string" length="50"></property>
<property name="subject" column="SUBJECT" type="text"></property>
<property name="tempContent" column="TEMPLATE_CONTENT" type="text"></property>
<property name="status" column="STATUS" type="integer"></property>
<property name="entryDate" column="ENTRY_DATETIME" type="timestamp"></property>
<property name="updateDatetime" column="UPDATE_DATETIME" type="timestamp"></property>
<property name="header" column="HEADER" type="text"></property>
<property name="footer" column="FOOTER" type="text"></property>
<property name="entryPerson" column="ENTRY_PERSON" type="integer"></property>
</class>
</hibernate-mapping>