<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.pack.passwordvo.ForgetpasswordTblVO" table="usr_reg_tbl">
		<id name="usrId" column="USR_ID" type="integer">
			<generator class="native"></generator>
		</id>
		<property name="emailId" column="EMAIL_ID" type="string" length="100"></property>
		<property name="pswd" column="PASSWD" type="string" length="100"></property>
		<property name="encryptPswd" column="ENCRYPT_PASSWD" type="string" length="100"></property>
		<property name="mobileno" column="MOBILE_NO" type="string" length="25"></property>		
		<property name="ivrBnPASSWORD_FLAG" column="PASSWORD_FLAG" type="integer" ></property>
		<property name="ivrBnStatus" column="ACT_STS" type="integer" ></property>
	</class>
</hibernate-mapping>